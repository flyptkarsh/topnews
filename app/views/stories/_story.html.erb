<div class="card"> 
  <div class="card-body">
    <h5 class="card-title">   <%= link_to s.title, s.link, html_options: { class: "card-link", target: '_blank' }%> </h5>
    <h6 class="card-subtitle mb-2 text-muted"><%=s.by%></h6>
    <p class="card-text">
        Published <%=time_ago_in_words(s.published_at)%> ago.
        <% if s.users.any? %>
          Starred by <%= s.users.map {|u| u.full_name }.to_sentence%>
        <% end %>
        <%= button_to "Star Story", story_stars_path(s), method: :post, class: "btn btn-primary float-end"%> 
    </p>
 


  </div> 
</div>